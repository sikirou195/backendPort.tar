
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql" // ou "postgresql", "sqlite", selon votre base
  url      = env("DATABASE_URL")
}
model Contact {
  id       Int    @id @default(autoincrement())
  name     String
  email    String
  message  String

  @@map("portofolio") // ðŸ‘‰ Dit Ã  Prisma que cette table s'appelle "portofolio" dans la DB
}

