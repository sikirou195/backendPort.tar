
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql" // ou "postgresql", "sqlite", selon votre base
  url      = env("DATABASE_URL")
}
model Contact {
  id       Int    @id @default(autoincrement())
  name     String
  email    String
   sujet    String
  message  String
 createdAt DateTime @default(now())
  @@map("portefeuille") // ðŸ‘‰ Dit Ã  Prisma que cette table s'appelle "portfeuile" dans la DB
}

